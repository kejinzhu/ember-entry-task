{
  "name": "ember-cli-htmlbars",
  "version": "3.0.1",
  "description": "A library for adding htmlbars to ember CLI",
  "main": "index.js",
  "files": [
    "index.js",
    "ember-addon-main.js",
    "utils.js"
  ],
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "yarn run test:ember && yarn run test:node",
    "test:ember": "ember test",
    "test:node": "mocha node-tests/*.js",
    "test:node:debug": "mocha debug node-tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ember-cli/ember-cli-htmlbars.git"
  },
  "keywords": [
    "ember-cli",
    "ember-addon"
  ],
  "ember-addon": {
    "main": "ember-addon-main.js",
    "configPath": "tests/dummy/config"
  },
  "author": "Jonathan Jackson & Chase McCarthy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ember-cli/ember-cli-htmlbars/issues"
  },
  "homepage": "https://github.com/ember-cli/ember-cli-htmlbars",
  "devDependencies": {
    "broccoli": "^2.0.0-beta.4",
    "broccoli-concat": "^3.5.1",
    "chai": "^4.2.0",
    "co": "^4.6.0",
    "ember-cli": "~3.3.0",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-shims": "^1.2.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.0",
    "ember-source": "~3.3.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-mocha": "^5.1.0",
    "eslint-plugin-node": "^7.0.1",
    "loader.js": "^4.2.3",
    "mocha": "^5.2.0",
    "qunit-dom": "^0.7.1"
  },
  "dependencies": {
    "broccoli-persistent-filter": "^1.4.3",
    "hash-for-dep": "^1.2.3",
    "json-stable-stringify": "^1.0.0",
    "strip-bom": "^3.0.0"
  }
}
